{
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": ["error", 2],
      "semi": ["error", "always"],
      "no-console": "off",
      "no-unused-vars": "warn",
      "no-warning-comments": "error"
    }
  }